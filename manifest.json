{
  "name": "LGTM",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "You can immediately paste the image of LGTM",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_popup": "html/popup.html"
  },
  "content_scripts": [
      {
          "matches": [
            "http://*/*", "https://*/*"
          ],
          "js": ["js/jquery-3.3.1.min.js", "js/contents.js"]
      }
  ]
}
